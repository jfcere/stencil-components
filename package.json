{
  "name": "stencil-sample",
  "version": "0.0.1",
  "description": "Sample repository using Stencil to build Web Components",
  "homepage": "https://github.com/jfcere/stencil-sample",
  "license": "MIT",
  "author": {
    "name": "Jean-Francois Cere",
    "email": "jfcere@sherweb.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jfcere/stencil-sample"
  },
  "keywords": [
    "stencil",
    "web components"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-sample/stencil-sample.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^1.16.0",
    "@stencil/sass": "^1.3.2",
    "@types/jest": "25.2.3",
    "@types/puppeteer": "2.0.1",
    "jest": "26.0.1",
    "jest-cli": "26.0.1",
    "puppeteer": "2.1.1"
  }
}
